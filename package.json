{
  "name": "@hfour/nestjs-json-rpc",
  "version": "4.1.1",
  "description": "JSON-RPC module for NestJS framework",
  "author": "hfour",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublish": "tsc -p tsconfig.json",
    "prettier:check": "prettier --no-color --check  \"src/**/*.ts\" \"test/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "yarn prettier:check && jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.5.1",
    "@nestjs/microservices": "^7.5.5",
    "@nestjs/platform-express": "^7.5.5",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.4.4",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/supertest": "^2.0.7",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "jest": "^25.1.0",
    "prettier": "^1.15.3",
    "rxjs": "^6.0.0",
    "supertest": "^6.0.1",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
